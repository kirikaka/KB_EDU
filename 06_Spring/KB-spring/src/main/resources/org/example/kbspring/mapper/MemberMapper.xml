<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--어느 인터페이스의 추상메서드에 이걸 할당할 건지-->
<mapper namespace="org.example.kbspring.mapper.MemberMapper">
    <select id="findAll" resultType="MemberEntity">
        select *
        from member;
    </select>
    <select id="findTwo" resultType="MemberEntity">
        select *
        from member LIMIT 2;
    </select>
</mapper>